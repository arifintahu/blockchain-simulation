{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "script"
    },
    "rules": {
        "linebreak-style": 0,
        "no-console": 0,
        "arrow-body-style": 0,
        "class-methods-use-this": 0,
        "max-classes-per-file": 0,
        "space-before-blocks": 0,
        "comma-dangle": 0,
        "no-plusplus": 0,
        "max-len": 0,
        "consistent-return": 0,
        "prefer-template": 0,
        "no-restricted-syntax": 0,
        "object-shorthand": 0,
        "no-else-return": 0,
        "no-return-await": 0,
        "radix": 0,
        "semi": 0,
        "array-callback-return": 0,
        "no-unneeded-ternary": 0,
        "no-useless-constructor": 0,
        "no-empty-function": 0,
        "no-trailing-spaces": 0
    }
}
